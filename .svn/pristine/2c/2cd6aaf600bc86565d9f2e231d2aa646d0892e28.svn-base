.jinengtisheng {
    width: 100%;
    margin-top: 20px;
    background-color: #FFFFFF;
}

.jinengtisheng ul {
    width: 100%;
    height: 250px;
}

.jinengtisheng ul li:hover {
    cursor: pointer;
    color: #C72E28;
}

.jinengtisheng ul li:nth-child(n+2) {
    margin: auto;
    width: 60%;
    height: 30px;
    font-size: 15px;
    line-height: 30px;
    text-align: left;
    background-color: #FFFFFF;
}

.jinengtisheng ul li:first-child {
    margin: auto;
    text-align: left;
    vertical-align: middle;
    width: 60%;
    height: 50px;
    border-bottom: 1px solid #797979;
    font-size: 18px;
    line-height: 50px;
    background-color: #FFFFFF;
}

.right {
    width: 25px;
    height: 20px;
    position: absolute;
    background-position: 0px 0px;
    background-image: url(../../img/userResume1/orderzhiping_right.jpg);
    left: 170px;
    background-color: #FFFFFF;
}

.right:hover {
    cursor: pointer;
}

.content-right {
    width: 80%;
    height: 100%;
    float: right;
    background-color: #FFFFFF;
}

.content-right>p:first-child{
    width: 100%;
    height: 40px;
    background-color: #F2F2F2;
    text-indent: 20px;
    font-size: 17px;
    line-height: 40px;
    border: 1px solid #DADADA;
    border-radius: 10px;
    color: #000;
}

.content-right>p:nth-child(2){
    width: 90%;
    height: 30px;
    font-size: 15px;
    margin: auto;
    margin-top: 40px;

}


.content-right>p:first-child span{
    text-align: left ;
    height: 40px;
    width: 100px;
}


.content-right>p:first-child span:nth-child(2) {
    color: #858784;
    margin-left: 15px;
}

.content-right>p:first-child span:last-child {
    border: 1px solid #a9a9a9;
    border-radius: 10px;
    color: #858784;
    float: right;
}

.table{
    margin: auto;
    margin-top: 10px;
    width: 90%;
    height: 100%;
    text-align: center;
    border-collapse: collapse;
    border: 1px solid #aeb0ad;
}

.table tr:nth-child(n+2){
    height: 50px;
}
.table tr:first-child {
    background-color:#d4e3e5;
}

.table td {
    border-width: 1px;
    padding: 8px;
    border-style: solid;
    border-color: #a9c6c9;
}

.table tr:nth-child(n+2) td:last-child span:hover{
    cursor: pointer;
    color: #C72E28;
}