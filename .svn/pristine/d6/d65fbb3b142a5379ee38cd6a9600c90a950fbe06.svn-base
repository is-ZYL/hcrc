
/*企业简介和企业形象*/
.dingdan-conter{
    width: 100%;
    height: 1100px;
    background-color: #F2F2F2;
}
.conter-left{
    width: 210px;
    height: 250px;
    background-color: #FFFFFF;
    margin-top: 20px;
    float: left;
}
.conter-right{
    width: 910px;
    height: 1050px;
    background-color: #FFFFFF;
    margin-top: 20px;
    float: right;
}
.conter-left p{
    margin-top:30px;
    margin-left: 30px;
}
.conter-left span{
    margin-left: 30px;
    font-size: 18px;
}
.conter-left p:nth-child(3)span{
    color: #E84626;
}
.right-top {
    width: 910px;
    height: 60px;
    background-color: #F2F2F2;
    border: 1px solid #DADADA;
}

.top-left{
    width: 266px;
    height: 58px;
    float: left;
}
.top-title{
    width: 131px;
    height: 58px;
    float: left;
    font-size: 17px;
    line-height: 58px;
    text-align: center;
}
.enterprise-image{
    width: 131px;
    height: 58px;
    float: right;
    font-size: 17px;
    line-height: 58px;
    text-align: center;
}


.top-right{
    width: 100px;
    height: 30px;
    background-color: #E84626;
    float: right;
    margin-top: 15px;
    margin-right: 15px;
}
.top-right span{
    line-height: 30px;
    text-align: center;
    display: inline-block;
    font-size: 15px;
    width: 100px;
    color: #ffffff;
}
.introduction{
    width: 910px;
    height: 950px;
}
/*简介  三个下划线框*/
.introduction-one-name{
    width: 809px;
    height: 213px;
    margin-left: 50px;
    border-bottom: 1px solid #cccccc;
}
.introduction-two-abbreviation{
    width: 809px;
    height: 255px;
    margin-left: 50px;
    border-bottom: 1px solid #cccccc;
}
.introduction-three-brief{
    width: 809px;
    height: 450px;
    margin-left: 50px;
    border-bottom: 1px solid #cccccc;
    position: relative;
}

.introduction-one-name ul li {
   display: inline-block;
}
.introduction-one-name ul li:nth-child(1){
    float: left;
    margin-top: 45px;
}
.introduction-one-name ul li:nth-child(2){
    margin-right: 230px;
    margin-top: 60px;
    float: right;
}
.introduction-one-name ul li:nth-child(2) p:nth-child(1){
    font-size: 20px;
}
.introduction-one-name ul li:nth-child(2) p{
    margin-top: 10px;
}

.introduction-two-abbreviation ul li{

    display: inline-block;
}
.introduction-two-abbreviation ul li:nth-child(1){
    margin-top: 50px;
    margin-left: 50px;
    font-size: 17px;
    text-align: right;
}
.introduction-two-abbreviation ul li:nth-child(2){
    margin-top: 50px;
    margin-left: 70px;
    font-size: 17px;
}
.introduction-two-abbreviation ul  li p{
    margin-top: 10px;
}

.introduction-three-brief ul li {
    display: inline-block;
}
.introduction-three-brief ul li:nth-child(1){
    position: absolute;
    top: 50px;
    margin-left: 50px;
    font-size: 17px;
}
.introduction-three-brief ul li:nth-child(2) p{
    width: 590px;
    height:350px;
    margin-left: 190px;
    margin-top: 50px;
}