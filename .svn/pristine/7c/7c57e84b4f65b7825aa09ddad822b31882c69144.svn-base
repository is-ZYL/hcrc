package com.entity;

public class Message {
	/**
	 * 消息
	 */
	private int m_id;
	
	private String m_type;
	
	private int m_who;
	
	private int m_to;
	
	private String m_content;

	public int getM_id() {
		return m_id;
	}

	public void setM_id(int m_id) {
		this.m_id = m_id;
	}

	public String getM_type() {
		return m_type;
	}

	public void setM_type(String m_type) {
		this.m_type = m_type;
	}

	public int getM_who() {
		return m_who;
	}

	public void setM_who(int m_who) {
		this.m_who = m_who;
	}

	public int getM_to() {
		return m_to;
	}

	public void setM_to(int m_to) {
		this.m_to = m_to;
	}

	public String getM_content() {
		return m_content;
	}

	public void setM_content(String m_content) {
		this.m_content = m_content;
	}

	@Override
	public String toString() {
		return "Message [m_id=" + m_id + ", m_type=" + m_type + ", m_who=" + m_who + ", m_to=" + m_to + ", m_content="
				+ m_content + "]";
	}
	
}
