.qiye-button{
    width: 100%;
    height: 1410px;
    background-color: #F2F2F2;
}
.button{
    width: 100%;
    height: 1500px;
}
.button-lunbo{
    width: 1140px;
    height: 320px;
}
.lunbo-left{
    width:790px;
    height: 320px;
    overflow: hidden;
    float: left;
    margin-top: 20px;
}
.reg-right{
    width: 330px;
    height: 320px;
    float: right;
    margin-top: 20px;
    background-color: #FFFFFF;
    position: relative;
}
.reg-right p:nth-child(1){
    position: absolute;
    top: 20px;
    left: 30px;
}
.reg-right p:nth-child(2) input{
    position: absolute;
    width: 260px;
    height: 30px;
    border: 1px solid #DDDDDD;
    top: 60px;
    left: 30px;
}
.reg-right p:nth-child(3) input{
    position: absolute;
    width: 260px;
    height: 30px;
    border: 1px solid #DDDDDD;
    top: 120px;
    left: 30px;
}
.reg-right p:nth-child(4) input{
    position: absolute;
    width: 20px;
    height: 20px;
    border: 1px solid #DDDDDD;
    top: 180px;
    left: 30px;
}
.reg-right p:nth-child(4) span{
    position: absolute;
    font-size: 20px;
    top: 175px;
    left: 80px;
}
.reg-right p:nth-child(1) span{
    font-size: 20px;
}
.reg-right p:nth-child(5) input{
    background-color: #666666;
    position: absolute;
    width: 120px;
    height: 40px;
    font-size: 20px;
    color: #ffffff;
    top: 220px;
    left: 30px;
}
.reg-right p:nth-child(5) input:nth-child(2){
    background-color: #E84626;
    position: absolute;
    width: 120px;
    height: 40px;
    top: 220px;
    left: 180px;

}
.reg-right p:nth-child(5) button{
    background-color: #E84626;
    position: absolute;
    width: 120px;
    height: 40px;
    font-size: 20px;
    color: #ffffff;
    top: 220px;
    left: 180px;
}
/*登录没写 上面*/
