    float: left;
    margin-top: 20px;
}
.talentpool{
    width: 330px;
    height: 360px;
    background: #ffffff;
    float: right;
    margin-top: 20px;
}
.search-xiala{
    position: relative;
}
.button-search li:nth-child(1) select{
    position: absolute;
    width: 100px;
    height: 36px;
    top: 20px;
    left: 30px;
    font-size: 14px;
    color: #A0A0A0;
    background-color: #F1F1F1;
}
.button-search li:nth-child(2) input{
    position: absolute;
    width: 410px;
    height: 35px;
    top: 20px;
    left: 150px;
    font-size: 14px;
    color: #A0A0A0;
    background-color: #F1F1F1;
    border: 1px solid #A0A0A0;
}
.button-search li:nth-child(3) input{
    position: absolute;
    width: 100px;
    height: 37px;
    top: 20px;
    left: 570px;
    font-size: 18px;
    color: #ffffff;
    background-color: #E84626;
}
.search-title{
    margin-top:70px;
    margin-left: 30px;
}
.search-title span{
    font-size: 15px;
}
.search-title a{
    margin-right: 10px;
}
.admission-top{
    width: 330px;
    height: 59px;
}
.admission-top a{
    margin-left: 30px;
    margin-top: 20px;
    font-size: 20px;
    float: left;
}
.admission-top img {
    margin-top: 25px;
    margin-right: 30px;
    float: right;
}
.button-admission hr{
    width: 270px;
}
.admission-down{
    position: relative;
}
.admission-down li{
    width: 330px;
    height: 70px;
    display: block;
}
.admission-down p{
    height: 70px;
    margin-left: 30px;
    display: inline-block;
    float: left;
}
.admission-down p:nth-child(2){
    float: right;
}
.admission-down ul li p:nth-child(2) span:nth-child(1){
    width: 210px;
    display: block;
}
.admission-down ul li p:nth-child(2) span:nth-child(3){
    margin-right: 50px;
    float: right;
}
.personnel-top{
    width: 790px;
    height: 60px;
    background-color: #F7F7F7;
}
.personnel-top-left{
    font-size: 20px;
    line-height: 60px;
    text-align: center;
    margin-left: 30px;
    float: left;
}
.personnel-top-left span:nth-child(2){
    margin-left: 30px;
}
.personnel-top-left span:nth-child(3){
    margin-left: 30px;
}

.personnel-top-right{
    margin-right: 30px;
    margin-top: 20px;
    float: right;
}
