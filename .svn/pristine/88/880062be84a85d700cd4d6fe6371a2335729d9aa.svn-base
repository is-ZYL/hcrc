.middle-content > div {
    border-radius: 10px;
}

.middle-content {
    width: 100%;
    height: 100%;
    margin-top: 20px;
    overflow: hidden;
    position: relative;
}

.middle-content .content {
    width: 1024px;
    height: 100%;
    position: relative;
    margin: auto;
}

.middle-content .content .content-left {
    width: 200px;
    height: 100%;
    float: left;
}

.content-left ul li {
    width: 100%;
    height: 50px;
    font-size: 18px;
    line-height: 50px;
    text-align: center;
}

.aa {
    background-color: #FFFFFF;
}

.content-left .aa > li:nth-child(-n+9):hover {
    background-color: #c5c5c5;
    color: #C72E28;
    cursor: pointer;
    border-radius: 10px;
}

