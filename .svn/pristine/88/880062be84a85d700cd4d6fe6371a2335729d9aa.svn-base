.jinengtisheng {
    width: 100%;
    margin-top: 20px;
    background-color: #FFFFFF;
}

.jinengtisheng ul {
    width: 100%;
    height: 250px;
}

.jinengtisheng ul li:hover {
    cursor: pointer;
    color: #C72E28;
}

.jinengtisheng ul li:nth-child(n+2) {
    margin: auto;
    width: 60%;
    height: 30px;
    font-size: 15px;
    line-height: 30px;
    text-align: left;
    background-color: #FFFFFF;
}

.jinengtisheng ul li:first-child {
    margin: auto;
    text-align: left;
    vertical-align: middle;
    width: 60%;
    height: 50px;
    border-bottom: 1px solid #797979;
    font-size: 18px;
    line-height: 50px;
    background-color: #FFFFFF;
}

.right {
    width: 25px;
    height: 20px;
    position: absolute;
    background-position: 0px 0px;
	background: url(../../img/userResume2/orderzhiping_right.jpg);
	left: 170px;
    background-color: #FFFFFF;
}

.right:hover {
    cursor: pointer;
}

.content-right {
    width: 80%;
    height: 100%;
    float: right;
    background-color: #FFFFFF;
}

.content-right > p:first-child {
    width: 100%;
    height: 40px;
    background-color: #F2F2F2;
    text-indent: 20px;
    font-size: 17px;
    line-height: 40px;
    border: 1px solid #DADADA;
    border-radius: 10px;
    color: #000;
}

.content-right > p:nth-child(2) {
    width: 90%;
    height: 30px;
    font-size: 15px;
    margin: auto;
    margin-top: 40px;

}

.content-right > p:first-child span {
    text-align: left;
    height: 40px;
    width: 100px;
}

.content-right > p:first-child span:nth-child(n+2) {
    color: #858784;
    margin-left: 15px;
}

.headers {
    width: 100%;
    height: 150px;
}

.headers > div:first-child {
    width: 90%;
    height: 100%;
    margin: auto;
}

.resumeInfo-left {
    width: 200px;
    height: 100%;
    float: left;
}

.resumeInfo-right {
    float: right;
}

.resumeInfo-left p {
    height: 20px;
    font-size: 15px;
    line-height: 20px;
    text-align: left;
    margin-top: 10px;
}

.resumeInfo-left p:first-child {
    font-size: 18px;
    font-weight: bolder;
    margin-top: 40px;
}

.resumeInfo-right ul li {
    margin-top: 50px;
    float: left;
    width: 70px;
    text-align: center;
    height: 100%;
}

.resumeInfo-right ul li:first-child {
    margin-left: 30px;
}

.resumeInfo-right ul li img {
    width: 35px;
    height: 35px;
    border-radius: 50%;
}

.resumeInfo-right ul li p {
    margin-top: 10px;
    height: 30px;
    font-size: 15px;
    line-height: 15px;
    text-align: center;
}

.otherInfo {
    width: 100%;
    height: 100%;
    margin: auto;
}

.otherInfo > ul {
    width: 100%;
    height: 100%;
}

.aaa {
    width: 100%;
    height: 50px;
    margin: auto;
    text-align: center;
    background-color: #F2F2F2;
    clear: both;
    padding-bottom: 10px;
}

.aaa div {
    width: 90%;
    height: 50%;
    padding-top: 15px;
    margin: auto;
}

.aaa span:first-child {
    font-size: 18px;
    color: #000;
    float: left;
}

.execute {
    width: 50px;
    font-size: 16px;
    color: #000;
    float: right;
    text-align: center;
    border: 1px solid #b8b8b8;
    border-radius: 5px;
}

.otherInfo > ul > li:nth-child(2n) {
    width: 90%;
    margin: auto;
}

.otherInfo > ul > li:nth-child(2n):nth-child(-n+2) span:nth-child(n+2) {
    margin-left: 5px;
}

.head_logo {
    width: 100px;
    height: 100px;
    border: 1px solid #858784;
    border-radius: 50%;
    float: left;
    margin-top: 20px;
}

.head_logo img {
    width: 100px;
    height: 100px;
    border: 1px solid #858784;
    border-radius: 50%;
}

.resume_basicInfo {
    float: left;
    margin-left: 20px;
    margin-top: 20px;
}

.resume_basicInfo p {
    font-size: 15px;
    width: 100%;
    height: 30px;
}

.resume_basicInfo p:first-child {
    font-size: 18px;
    color: #000;
}

.myIntroduce {
    text-align: left;
    padding-top: 30px;
    padding-bottom: 30px;
}

.workExperience > li {
    width: 100%;
    height: 100%;
    border-bottom: 1px solid #9a9a9a;

}

.workExperience > li:last-child {
    border-bottom: none;
}

.workExperience li p:first-child {
    padding-top: 15px;
    padding-bottom: 15px;
    height: 20px;
}

.workExperience li p:first-child span:nth-child(n+1):nth-child(-n+5) {
    font-size: 16px;
    color: #000;
}

.workExperience li p:last-child {
    padding-top: 15px;
    padding-bottom: 30px;

}

.eduExperience li p:last-child {
    clear: both;
    padding-bottom: 30px;

}

.projectExperience li p:nth-child(2) {
    height: 20px;
    font-size: 17px;
    color: #000;
}

.otherSpecialties li p {
    font-size: 16px;
    color: #000;
}

.personalSpace li p {
    font-size: 15px;
    color: #000;
}

.softwareSkills {
    clear: both;
    margin-top: 10px;
    width: 100%;
    height: 100%;
}

.softwareSkills li {
    clear: both;
    width: 100%;
    height: 100%;
    padding-top: 20px;
    padding-bottom: 20px;
}

.softwareSkills li:last-child{
padding-bottom: 40px;
}
.softwareSkills > li > p {
    clear: both;
    width: 100%;
    height: 100%;
}

.softwareSkills span {
    float: left;
    line-height: 15px;
    font-size: 16px;
    font-style: inherit;
    color: #999999;

}

.skillfulBar {
    margin-left: 100px;
    height: 10px;
    width: 480px;
    border: 1px solid #d3d3d3;
    margin-top: 2px;
}

.skillfulBar > span {
    height: 100%;
    width: 300px;
    background-color: #a3a3a3;
}

.skillfulBar + span {
    float: right;
}